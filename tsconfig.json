{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@dtos/*": ["./src/dtos/*"],
      "@assets/*": ["./assets/*"],
      "@components/*": ["./source/components/*"],
      "@screens/*": ["./source/screens/*"],
      "@storage/*": ["./source/storage/*"],
      "@utils/*": ["./source/utils/*"],
      "@services/*": ["./source/services/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@contexts/*": ["./src/contexts/*"],
      "@routes/*": ["./src/routes/*"]
    }
  }
}
